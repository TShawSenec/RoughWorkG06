<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
        <LinearLayout
            app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/nav_bar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:weightSum="3"
            android:layout_gravity="center"
            android:background="@color/navbarC">
        <ImageButton
            android:layout_weight="1"
            android:id="@+id/backbutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="?attr/homeAsUpIndicator"
            android:backgroundTint="@color/white"
            android:layout_margin="5pt"
            android:background="?attr/selectableItemBackgroundBorderless"/>
        <TextView
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:textAlignment="center"
            android:textColor="@color/orange"
            android:text="Fitness Tracker"
            ></TextView>
        <ImageButton
            android:layout_weight="1"
            android:id="@+id/nextbutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="?attr/homeAsUpIndicator"
            android:backgroundTint="@color/white"
            android:layout_margin="5pt"
            android:scaleX="-1"
            android:background="?attr/selectableItemBackgroundBorderless"/>
        </LinearLayout>
    <LinearLayout
        app:layout_constraintTop_toBottomOf="@+id/nav_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:clickable="true"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@color/white"
            android:weightSum="4"
            android:layout_marginBottom="5dp">
            <ImageView
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_image_black_24dp"/>
            <TextView
                android:layout_weight="1"
                android:layout_width="50pt"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textAlignment="center"
                android:text="Workout 1"/>
            <ImageView
                android:layout_gravity="fill_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_close_black_24dp"/>
            <ImageView
                android:layout_gravity="fill_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_play_arrow_black_24dp"
                />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:clickable="true"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@color/white"
            android:weightSum="4"
            android:layout_marginBottom="5dp">
            <ImageView
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_image_black_24dp"/>
            <TextView
                android:layout_weight="1"
                android:layout_width="50pt"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textAlignment="center"
                android:text="Workout 2"/>
            <ImageView
                android:layout_gravity="fill_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_close_black_24dp"/>
            <ImageView
                android:layout_gravity="fill_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_play_arrow_black_24dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:clickable="true"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@color/white"
            android:weightSum="4"
            android:layout_marginBottom="5dp">
            <ImageView
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_image_black_24dp"/>
            <TextView
                android:layout_weight="1"
                android:layout_width="50pt"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textAlignment="center"
                android:text="Workout 3"/>
            <ImageView
                android:layout_gravity="fill_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_close_black_24dp"/>
            <ImageView
                android:layout_gravity="fill_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_play_arrow_black_24dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:clickable="true"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@color/white"
            android:weightSum="4"
            android:layout_marginBottom="5dp">
            <ImageView
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_image_black_24dp"/>
            <TextView
                android:layout_weight="1"
                android:layout_width="50pt"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textAlignment="center"
                android:text="Workout 4"/>
            <ImageView
                android:layout_gravity="fill_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_close_black_24dp"/>
            <ImageView
                android:layout_gravity="fill_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_play_arrow_black_24dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:clickable="true"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@color/white"
            android:weightSum="4"
            android:layout_marginBottom="5dp">
            <ImageView
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_image_black_24dp"/>
            <TextView
                android:layout_weight="1"
                android:layout_width="50pt"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textAlignment="center"
                android:text="Workout 5"/>
            <ImageView
                android:layout_gravity="fill_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_close_black_24dp"/>
            <ImageView
                android:layout_gravity="fill_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_play_arrow_black_24dp"/>
        </LinearLayout>
        <LinearLayout

            android:layout_width="match_parent"
            android:clickable="true"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@color/white"
            android:weightSum="4"
            android:layout_marginBottom="5dp">
            <ImageView
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_image_black_24dp"/>
            <TextView
                android:layout_weight="1"
                android:layout_width="50pt"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textAlignment="center"
                android:text="Workout 6"/>
            <ImageView
                android:layout_gravity="fill_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_close_black_24dp"/>

            <ImageView
                android:layout_gravity="fill_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_play_arrow_black_24dp"/>
        </LinearLayout>
        <LinearLayout

            android:layout_width="match_parent"
            android:clickable="true"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:background="@color/white"
            android:weightSum="4"
            android:layout_marginBottom="5dp">
            <ImageView
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_image_black_24dp"/>
            <TextView
                android:layout_weight="1"
                android:layout_width="50pt"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textAlignment="center"
                android:text="Workout 7"/>
            <ImageView
                android:layout_gravity="fill_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_close_black_24dp"/>

            <ImageView
                android:layout_gravity="fill_vertical"
                android:layout_weight="1"
                android:layout_width="25pt"
                android:layout_height="25pt"
                android:src="@drawable/ic_play_arrow_black_24dp"/>
        </LinearLayout>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>